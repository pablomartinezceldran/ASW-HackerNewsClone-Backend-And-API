<!DOCTYPE html>
<html lang="">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    />
    <link
            rel="stylesheet"
            href="/css/navbar.css"
    />
    <title>Hacker News</title>
  </head>


  <body>
  <div class="container mt-2">
    <nav class = "navFormat">
      <a class="navbar-brand" href="/">
        <img src="/images/y18.jpeg" width="18" height="18" style="border:1px #ffffff solid;padding-right: 0px;">
      </a>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="/submit" style="padding-left: 0px;">
              <td style="line-height:12px; height:10px;">
                                  <span class="pagetop">
                                      <b style="color: #000000;">Submit</b>
                                  </span>
              </td>
            </a>
          </li>
        </ul>
      </div>
    </nav>
  </div>
        <% submissions.forEach( (submission)=>{ %>
          <tr>
              <td>
                <%= submission.title%> y el url <%= submission.url%> <br> 
                <li class="nav-item">
                  <a class="nav-link" href="/submission/<%= submission._id %>"" > <%= submission.title%> </a>   
                  <%= submission.votes %> <--votes
                </li>
                <br>
                <br>
              </td>
          </tr>
          <% }) %>
  </body>
</html>