<!DOCTYPE html>
<html lang="">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    />
    <link rel="stylesheet" href="/css/navbar.css"/>
    <title>Hacker News</title>
  </head>


  <body>
  <table class="hdMain">
    <tbody>
    <tr>
      <td class="table-color">
        <table class="header">
          <tbody>
          <tr>
            <td class="tdImage">
              <a class="navbar-brand" href="/">
                <img src="/images/y18.jpeg" width="18" height="18" style="border:1px #ffffff solid;padding-right: 0px;">
              </a>
            </td>
            <td class = "nav-links">
              <a class="nav-link" href="/" style="padding-left: 0px;">
              <span class="page-top">
                <b class="navText">Hacker News</b>
              </span>
              </a>
            </td>
            <td class = "nav-links">
              <a class="nav-link" href="/newest" style="padding-left: 2px; padding-right: 2px;">
              <span class="page-top">
                <d class="navText-Current">new</d>
              </span>
              </a>
            </td>
            <td class = "navText">
              |
            </td>
            <td class = "nav-links">
              <a class="nav-link" href="/submit" style="padding-left: 2px; padding-right: 2px;">
              <span class="page-top">
                <d class="navText">submit</d>
              </span>
              </a>
            </td>
          </tr>
          </tbody>
        </table>
      </td>
    </tr>
      <% submissions.forEach( (submission)=>{ %>
          <tr>
              <td>
                <%= submission.title%> y el url <%= submission.url%> <br> 
                <li class="nav-item">
                  <a class="nav-link" href="/submission/<%= submission._id %>"" > <%= submission.title%></a>
                </li>
                <br>
                <br>
              </td>
          </tr>
          <% }) %>
    </tbody>
  </table>
  </body>
</html>